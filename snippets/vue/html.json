{
  "v-if": {
    "prefix": "vif",
    "body": ["v-if=\"${1}\" "],
    "description": "v-if"
  },
  "v-else": {
    "prefix": "vel",
    "body": ["v-else "],
    "description": "v-else"
  },
  "v-else-if": {
    "prefix": "velif",
    "body": ["v-else-if=\"${1}\" "],
    "description": "v-else-if"
  },
  "v-model": {
    "prefix": "vmo",
    "body": ["v-model=\"${1:value}\" "],
    "description": "v-model"
  },
  "v-model.lazy": {
    "prefix": "vmol",
    "body": ["v-model.lazy=\"${1:value}\" "],
    "description": "v-model.lazy"
  },
  "v-model.number": {
    "prefix": "vmon",
    "body": ["v-model.number=\"${1:value}\" "],
    "description": "v-model.number"
  },
  "v-model.trim": {
    "prefix": "vmot",
    "body": ["v-model.trim=\"${1:value}\" "],
    "description": "v-model.trim"
  },
  "v-for (index,item)": {
    "prefix": "vfi",
    "body": [
      "v-for=\"(${2:item}, ${4:index}) in ${1:items}\" :key=\"${3:index}\" "
    ],
    "description": "v-for"
  },
  "v-for item": {
    "prefix": "vf",
    "body": ["v-for=\"${2:item} in ${1:items}\" :key=\"${2:item}\" "],
    "description": "v-for"
  },
  "@click handle": {
    "prefix": "chandle",
    "body": ["@click=\"handle${1:Click}\" "],
    "description": "@click handle"
  }
}
